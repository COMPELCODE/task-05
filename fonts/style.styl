@import "variables"

@font-face
  font-family 'my-itXiuZhen'
  src url(icomoon-font-path + '/my-itXiuZhen.eot?gcytiq')
  src url(icomoon-font-path + '/my-itXiuZhen.eot?gcytiq#iefix') format('embedded-opentype'), url(icomoon-font-path + '/my-itXiuZhen.ttf?gcytiq') format('truetype'), url(icomoon-font-path + '/my-itXiuZhen.woff?gcytiq') format('woff'), url(icomoon-font-path + '/my-itXiuZhen.svg?gcytiq#my-itXiuZhen') format('svg')
  font-weight normal
  font-style normal

[class^="icon-xz-"], [class*=" icon-xz-"]
  font-family 'my-itXiuZhen' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-xz-location
  &:before
    content icon-xz-location
.icon-xz-star
  &:before
    content icon-xz-star
.icon-xz-angle-left
  &:before
    content icon-xz-angle-left

